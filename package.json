      "path": "dist/rnutilshooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/react": "^18.2.15",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "react": "^18.2.0",
    "size-limit": "^8.2.6",
    "ts-jest": "^29.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
